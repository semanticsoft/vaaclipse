<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="org.semanticsoft.vaaclipse.demo.contacts"
            application="org.semanticsoft.vaaclipse.app.application">
         <property
               name="clearPersistedState"
               value="true">
         </property>
          <property
               name="applicationXMI"
               value="org.semanticsoft.vaaclipse.demo.contacts/Application.e4xmi">
         </property>
          <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="cssTheme"
               value="reindeer">
         </property>
         <property
               name="appName"
               value="org.semanticsoft.vaaclipse.demo.contacts">
         </property>
          <property
           name="lifeCycleURI"
           value="bundleclass://org.semanticsoft.vaaclipse.demo.contacts/org.semanticsoft.vaaclipse.demo.lifecycle.LifecycleHandler">
         </property>
      </product>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
